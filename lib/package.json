{
  "name": "victory-native",
  "version": "40.0.0",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/victory-native-xl"
  },
  "react-native": "src/index",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
